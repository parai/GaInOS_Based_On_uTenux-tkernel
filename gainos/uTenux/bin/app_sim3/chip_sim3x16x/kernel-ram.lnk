OUTPUT_FORMAT("elf32-littlearm", "elf32-bigarm", "elf32-littlearm")
OUTPUT_ARCH(arm)

SECTIONS
{
	.init 0x20080000 : {
		*(.vector)
		*(.ftext)
		*(.ftext.*)
	}

	.text 0x20000000 : {
		*(.text)
		*(.rodata)
	} =0
	 __exidx_start = .;
    .ARM.exidx :
    {
        *(.ARM.exidx* .gnu.linkonce.armexidx.*)
    } = 0
    __exidx_end = .;
	__data_org = . ;
	.data : {
		__data_start = . ;
		*(.data)
		__data_end = .;
	}
	.bss : {
		__bss_start = .;
		PROVIDE (__noinit_start = .);
		*(.noinit)
		PROVIDE (__noinit_end = .);
		*(.bss)
		*(COMMON)
		__bss_end = .;
	}

	/DISCARD/ : {
		*(.romtext.*)
	}
}
